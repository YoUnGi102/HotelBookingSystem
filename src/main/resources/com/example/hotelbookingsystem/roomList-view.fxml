<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="480.0" prefWidth="720.0" spacing="20.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.assignment1.view.TemperatureViewController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <TableView fx:id="tableView" prefHeight="371.0" prefWidth="680.0">
     <columns>
       <TableColumn fx:id="numberColumn" prefWidth="90.0" text="#" />
         <TableColumn fx:id="available" prefWidth="200.0" text="Available" />
         <TableColumn fx:id="booked" prefWidth="200.0" text="Booked" />
         <TableColumn fx:id="outOfOrder" prefWidth="200.0" text="Out Of Order" />
     </columns>
   </TableView>
   <HBox prefHeight="100.0" prefWidth="680.0">
      <children>
         <Button fx:id="backBtn" mnemonicParsing="false" onAction="#back" prefHeight="80.0" prefWidth="100.0" text="BACK">
            <HBox.margin>
               <Insets right="10.0" top="35.0" />
            </HBox.margin>
            <font>
               <Font size="20.0" />
            </font>
         </Button>
         <Label fx:id="errorLabel" alignment="CENTER" prefHeight="100.0" prefWidth="150.0" textAlignment="CENTER" textFill="RED">
            <HBox.margin>
               <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </HBox.margin>
         </Label>
         <VBox prefHeight="71.0" prefWidth="150.0">
            <children>
               <HBox prefHeight="100.0" prefWidth="200.0" />
            </children>
         </VBox>
         <VBox prefHeight="71.0" prefWidth="150.0">
            <HBox.margin>
               <Insets left="20.0" right="20.0" />
            </HBox.margin>
         </VBox>
         <Button fx:id="saveBtn" mnemonicParsing="false" onAction="#saveTemperatures" prefHeight="54.0" prefWidth="104.0" text="SEARCH">
            <HBox.margin>
               <Insets bottom="5.0" top="40.0" />
            </HBox.margin>
            <padding>
               <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
            </padding>
         </Button>
      </children>
   </HBox>
</VBox>
